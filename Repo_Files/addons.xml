<addon id="plugin.video.streamhub" name="[B][COLOR red]Stream Hub[/COLOR][/B]" version="1.9" provider-name="sClarke">
	<requires>
		<import addon="xbmc.python" version="2.1.0" />
		<import addon="plugin.video.youtube" version="5.2.1" optional="true" />
		<import addon="script.module.pyxbmct" version="1.1.4" />
		<import addon="script.module.urlresolver" version="3.0.0"/>
		<import addon="script.module.liveresolver" version="0.1.24"/>
		<import addon="script.video.F4mProxy" version="2.2.3"/>
        <import addon="script.streamhub.artwork"/>
        <import addon="script.streamhub.metadata"/>
        <import addon="script.module.streamhub"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en"></summary>
		<description lang="en">StreamHub - All Content feautured in this Addon is freely Available on the Internet - Making use of Already Written Code and Writing our own. Make sure you Join our facebook page for News and Support. http://facebook.com/groups/streamh  - Credit to Lambda/Exodus For Movies and TV Shows. Credit to Phoenix for the Addon's Base Code</description>
		<platform>all</platform>
	</extension>
</addon>

