<addons>
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.streamhub" name="[COLOR red][B]StreamHub[/COLOR][/B]" version="2.3" provider-name="sClarke">
	<requires>
		<import addon="xbmc.python" version="2.1.0" />
		<import addon="plugin.video.youtube" version="5.2.1" optional="true" />
		<import addon="script.module.pyxbmct" version="1.1.4" />
		<import addon="script.module.urlresolver" version="3.0.0"/>
		<import addon="script.module.liveresolver" version="0.1.24"/>
		<import addon="script.video.F4mProxy" version="2.2.3"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">StreamHub</summary>
		<description lang="en">Bringing Movies, TV shows, Kids Content and More!</description>
		<platform>all</platform>
		<assets>
			<icon>icon.png</icon>
			<fanart>fanart.jpg</fanart>
			<screenshot>resources/screenshots/screenshot-01.jpg</screenshot>
			<screenshot>resources/screenshots/screenshot-02.jpg</screenshot>
			<screenshot>resources/screenshots/screenshot-03.jpg</screenshot>
		</assets>
		<news>New Premium Sports Section!</news>
	</extension>
</addon>

<addon id="plugin.video.torba" name="[B][COLOR yellow]Torba[/COLOR][/B]" version="1.2" provider-name="sClarke">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.requests" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Torba.se</summary>
    <description lang="en">All content is found at torba.se, we do not host or own any of the content.</description>
	<disclaimer lang="en">Torba.se</disclaimer>
    <platform>all</platform>
  </extension>
</addon>



<addon id="script.module.streamhub" name="[B][COLOR red]Stream Hub[/COLOR][/B]" version="3.2" provider-name="sClarke">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.requests" />
	<import addon="script.module.urlresolver" />
	<import addon="script.module.liveresolver" version="0.1.24"/>
